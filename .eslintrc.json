{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-console": "off",
        "max-len": "off",
        "lines-between-class-members": "off",
        "object-curly-newline": "off",
        "import/no-import-module-exports": "off",
        "no-restricted-syntax": "off",
        "indent": "off"
    }
}